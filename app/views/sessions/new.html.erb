
<div class="login-container">
    <%= form_tag sessions_path do %>
        <%= label_tag :name %><br>
        <%= text_field_tag :name, params[:name] %><br>
        <br>
        <%= label_tag :password %><br>
        <%= password_field_tag :password %><br>
        <br>
        <%= submit_tag 'Login' %>
    <% end %>
</div>
